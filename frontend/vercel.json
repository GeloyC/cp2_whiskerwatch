{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/node",
            "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/index.html"
        }
    ]
}