{
    "version": 2,
    "builds": [
        {
        "src": "frontend/package.json",
        "use": "@vercel/static",
        "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/dist/$1"
        },
        {
        "src": "/",
        "dest": "/dist/index.html"
        }
    ]
}